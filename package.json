  "license": "MIT",
  "dependencies": {
    "nid": "0.3",
    "norma": "0.4",
    "eraro": "1.0"
  },
  "devDependencies": {
    "coveralls": "3",
    "lab": "14",
    "prettier": "1"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "use.js"
  ]
