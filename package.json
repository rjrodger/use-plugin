{
  "name": "use-plugin",
  "version": "1.0.2",
  "description": "Generic plugin loader functionality for Node.js frameworks.",
  "main": "use.js",
  "scripts": {
    "test": "lab -v test/use.test.js -t 85 -I SharedArrayBuffer,Atomics,URL,URLSearchParams",
